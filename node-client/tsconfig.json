{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es6",
        "moduleResolution": "node",
        "removeComments": false,
        "sourceMap": true,
        "noLib": false,
        "declaration": true,
        "outDir": "dist"
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "include": [
        "*.ts",
        "src/**/*"
    ]
}

